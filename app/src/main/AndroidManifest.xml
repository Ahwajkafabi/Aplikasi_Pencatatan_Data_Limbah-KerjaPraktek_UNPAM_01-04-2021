<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ahwajkafabi.pencatatandatalimbah">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/sinarmas"
        android:label="@string/app_name"
        android:roundIcon="@drawable/sinarmas"
        android:supportsRtl="true"
        android:theme="@style/Theme.PencatatanDataLimbah">
        <activity android:name=".DetailActivity"></activity>
        <activity android:name=".CekhlistActivity" />
        <activity android:name=".CRUDActivity" />
        <activity android:name=".DasboardActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>